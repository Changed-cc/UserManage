<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>UserManage</display-name>
  <servlet>
    <servlet-name>LoginController</servlet-name>
    <servlet-class>edu.cd.controller.LoginController</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>RegisterController</servlet-name>
    <servlet-class>edu.cd.controller.RegisterController</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>UserController</servlet-name>
    <servlet-class>edu.cd.controller.UserController</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>Login</servlet-name>
    <servlet-class>edu.cd.view.Login</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>Main</servlet-name>
    <servlet-class>edu.cd.view.Main</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>Register</servlet-name>
    <servlet-class>edu.cd.view.Register</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>RegisterSuccess</servlet-name>
    <servlet-class>edu.cd.view.RegisterSuccess</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>Manage</servlet-name>
    <servlet-class>edu.cd.view.Manage</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>SearchView</servlet-name>
    <servlet-class>edu.cd.view.ManageAction.SearchView</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>SearchSuccess</servlet-name>
    <servlet-class>edu.cd.view.ManageAction.SearchSuccess</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>LoginController</servlet-name>
    <url-pattern>/LoginController</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>RegisterController</servlet-name>
    <url-pattern>/RegisterController</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>UserController</servlet-name>
    <url-pattern>/UserController</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Login</servlet-name>
    <url-pattern>/Login</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Main</servlet-name>
    <url-pattern>/Main</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Register</servlet-name>
    <url-pattern>/Register</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>RegisterSuccess</servlet-name>
    <url-pattern>/RegisterSuccess</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Manage</servlet-name>
    <url-pattern>/Manage</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>SearchView</servlet-name>
    <url-pattern>/SearchView</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>SearchSuccess</servlet-name>
    <url-pattern>/SearchSuccess</url-pattern>
  </servlet-mapping>


  <welcome-file-list>
    <welcome-file>Login</welcome-file>
  </welcome-file-list>
</web-app>